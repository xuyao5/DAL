<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <files>
        <file>
            <id>file1</id><!-- 唯一ID -->
            <name>测试文件1</name><!-- 名称 -->
            <enable>true</enable><!-- 开关 -->
            <path>/Users/xuyao/Downloads/test.txt</path><!-- 源路径 -->
